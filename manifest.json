{
  "name": "noNewsBbc",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Skip news sections",
  "homepage_url": "http://normally.com",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Skip News"
  },
  "permissions": [
    "https://www.bbc.co.uk/",
    "tabs"
  ]
}