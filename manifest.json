{
  "name": "noNewsBbc",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Skip news sections",
  "homepage_url": "http://normally.com",
  "icons": { 
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png" 
  },
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Skip News"
  },
  "permissions": [
    "https://www.bbc.co.uk/",
    "tabs"
  ]
}